{
  "AppInfo": {
    "Env": "prod",
    "Addr": ":3000"
  },
  "Log": {
    "LogBasePath": "/app/log/",
    "LogFileName": "app"
  },
  "Wechat": {
    "AppID": "<appid>",
    "AppSecret": "<app secret>",
    "RedirectURI": "https://<weixin host>/weixin-login/api/v1/weixin/code",
    "ResponseType": "snsapi_userinfo"
  },
  "URL": {
    "CodeURL": "https://open.weixin.qq.com/connect/oauth2/authorize?appid=%s&redirect_uri=%s&response_type=code&scope=%s&state=%s#wechat_redirect",
    "TokenURL": "https://api.weixin.qq.com/sns/oauth2/access_token",
    "RefreshURL": "https://api.weixin.qq.com/sns/oauth2/refresh_token",
    "UserInfoURL": "https://api.weixin.qq.com/sns/userinfo",
    "WhiteList": [
      "<host>",
      "<another host>"
    ]
  },
  "Redis": {
    "Host": "redis",
    "Port": "6379"
  }
}